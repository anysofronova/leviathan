{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "format",
          "typecheck",
          "lint",
          "test",
          "test:cov",
          "test:debug",
          "test:e2e",
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test:cov": {
      "outputs": [
        "{projectRoot}/coverage"
      ]
    }
  },
  "defaultBase": "develop"
}
